# Docker engine
alias dc="docker"
alias dr="dc run"
alias ds="dc stop"
alias dim="dc images"
alias drmi="dc rmi"
alias drm="dc rm"
alias dcx="dc exec"
alias dcp="dc ps"
alias dcpa="dc ps -a"
alias di="dc inspect"
alias dcv="dc volume"
alias dip="dc inspect -f '{{ .NetworkSettings.IPAddress }}'"

# Docker Swarm
alias dsd="docker stack deploy"

# Docker machine
alias dm="docker-machine"
alias dml="dm ls"
